        }
    ],
    "support": {
        "issues": "https://github.com/unematiii/wp-shortcode-tree/issues",
        "source": "https://github.com/unematiii/wp-shortcode-tree/tree/master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5",
        "wp-coding-standards/wpcs": "^2.3"
